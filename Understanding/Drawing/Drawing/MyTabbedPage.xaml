            xmlns:local="clr-namespace:Drawing"
            x:Class="Drawing.MyTabbedPage">

    <NavigationPage Title="A">
        <x:Arguments>
            <local:CirclePage Title="CirclePage"/>
        </x:Arguments>
    </NavigationPage>

    <NavigationPage Title="B">
        <x:Arguments>
            <local:MyCirclePage Title="MyCirclePage"/>
        </x:Arguments>
    </NavigationPage>

</TabbedPage>
