<?xml version="1.0" encoding="utf-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:TheTranslateTransform"
            x:Class="TheTranslateTransform.MyTabbedPage">

    <NavigationPage Title="A">
        <x:Arguments>
            <local:AccumulatedTranslatePage Title="Accumulated Translate"/>
        </x:Arguments>
    </NavigationPage>

    <NavigationPage Title="B">
        <x:Arguments>
            <local:TranslateTextEffectsPage Title="Translate Text Effects"/>
        </x:Arguments>
    </NavigationPage>

    <NavigationPage Title="C">
        <x:Arguments>
            <local:HendecagramArrayPage Title="Hendecagram Array"/>
        </x:Arguments>
    </NavigationPage>

    <NavigationPage Title="D1">
        <x:Arguments>
            <local:HendecagramAnimationPage Title="Hendecagram Animation 1"/>
        </x:Arguments>
    </NavigationPage>

    
    <NavigationPage Title="D2">
        <x:Arguments>
            <local:HendecagramAnimation2Page Title="Hendecagram Animation 2"/>
        </x:Arguments>
    </NavigationPage>

</TabbedPage>